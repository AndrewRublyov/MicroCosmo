let a : int

func fact(val : int) : int { 
    if (val <= 1) { 
        return 1
    }
    
    return (val * fact(val - 1))
} 

func main1() {
    a = 1
    while (a < 6) {
        printlni(fact(a))
        a = a + 1
    }
}

